 { document.getElementById("video_2").style.backgroundImage="url('pages/video2.jpg')";
  clearInterval(myVar);
 }
}
sessionStorage.setItem('key',"red_2");
sessionStorage.setItem('lock',"mutex_unlock_2");
function myfunction_1() {
var elem=document.getElementById("idvid_2");   
       if(sessionStorage.getItem('key')=="green_2")
   {
       elem.webkitExitFullscreen();
             elem.load();
       sessionStorage.setItem('lock',"mutex_unlock_2");
   }
     if (sessionStorage.getItem('key')=="red_2")
          {          elem.webkitRequestFullScreen();   
    elem.play();
    sessionStorage.setItem('key', "green_2");
         } 
}

function test_1(){
 if((event.key == "Enter") && ((document.getElementById("ans_2").value == "yes")) && (sessionStorage.getItem('lock') == "mutex_unlock_2"))
     {  document.getElementById("mem_2").innerHTML= "I am taking you to my  second year , I saw the beast with my own eyes..";   
        setTimeout(function(){document.getElementById("mem_2").innerHTML= "Just tap the basilisk";},3000);
        document.getElementById("idvid_2").setAttribute('poster','anim/snake.gif');     
            sessionStorage.setItem('lock',"mutex_lock_2");
   
          }
}
</script>
</div>

