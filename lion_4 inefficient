var myVar = setInterval(magic, 3000);
function magic() {
var temp=sessionStorage.getItem('p');
if(temp == "po")
 { document.getElementById("video_20").style.backgroundImage="url('pages/video20.jpg')";
  clearInterval(myVar);
 }
}

function myfunction_1() {
var elem=document.getElementById("idvid_20");
if(!elem.webkitFullscreenEnabled)
   {
       elem.webkitExitFullscreen();
       elem.load();
   }
     if (elem.webkitRequestFullscreen)
         { elem.webkitRequestFullScreen();
          }
setTimeout(function(){ 
document.getElementById("mem_20").innerHTML= "I have a memory I can show you!";
document.getElementById("idvid_20").setAttribute('poster','anim/blank.jpg');
var new_input = document.createElement("input");
new_input.setAttribute("id", "ans_20");
new_input.setAttribute("type","text");
var enclosing = document.getElementById("video_20");
enclosing.appendChild(new_input);
 }, 3000);
document.getElementById("ans_20").addEventListener("keypress",test_1,false);
}

function test_1(){
 if((event.key == "Enter") && ((document.getElementById("ans_20").value == "yes")))
     {  document.getElementById("mem_20").innerHTML= "My second challenge of tri wizrd cup!";   
        setTimeout(function(){document.getElementById("mem_20").innerHTML= "tap the fishes ";},3000);
        document.getElementById("idvid_19").setAttribute('poster','anim/fishes.gif');     
        var element = document.getElementById("ans_20");
         element.parentNode.removeChild(element);
  
        }
}
</script>
</div>

