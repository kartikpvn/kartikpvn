</span>
<video poster = "anim/blank.png"  onclick="myfunction_1();" align="top|middle" width="500" height="500"  id="idvid_19">
<source src="videos\dimentor.mp4" type="video/mp4">
</video>
<input id="ans_19" type="text" onkeypress="test_1()">
<script type="text/javascript">
var myVar = setInterval(magic, 3000);
function magic() {
var temp=sessionStorage.getItem('p');
if(temp == "po")
 { document.getElementById("video_19").style.backgroundImage="url('pages/video19.jpg')";
  clearInterval(myVar);
 }
}

function myfunction_1() {
var elem=document.getElementById("idvid_19");
if(!elem.webkitFullscreenEnabled)
   {
       elem.webkitExitFullscreen();
       elem.load();
   }
     if (elem.webkitRequestFullscreen)
         { elem.webkitRequestFullScreen();
          }
setTimeout(function(){ 
document.getElementById("mem_19").innerHTML= "I have a memory I can show you!";
document.getElementById("idvid_19").setAttribute('poster','anim/blank.jpg');
var new_input = document.createElement("input");
new_input.setAttribute("id", "ans_19");
new_input.setAttribute("type","text");
var enclosing = document.getElementById("video_19");
enclosing.appendChild(new_input);
 }, 3000);
document.getElementById("ans_19").addEventListener("keypress",test_1,false);
}

function test_1(){
 if((event.key == "Enter") && ((document.getElementById("ans_19").value == "yes")))
     {  document.getElementById("mem_19").innerHTML= "my first encounter with a dimentor in the train!";   
        setTimeout(function(){document.getElementById("mem_19").innerHTML= "Just the ghost ";},3000);
        document.getElementById("idvid_19").setAttribute('poster','anim/ghost.gif');     
        var element = document.getElementById("ans_19");
         element.parentNode.removeChild(element);
  
        }
}
</script>
</div>

