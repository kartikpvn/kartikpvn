</span>
<video poster = "anim/blank.png"  onclick="myfunction_1();" align="top|middle" width="150" height="150"  id="idvid_1">
<source src="videos\acromantula.mp4" type="video/mp4">
</video>
<input id="ans_1" type="text" onkeypress="test_1()">
<script>
var myVar = setInterval(magic, 3000);
function magic() {
var temp=sessionStorage.getItem('p');
if(temp == "po")
 { document.getElementById("video_1").style.backgroundImage="url('pages/video1.jpg')";
  clearInterval(myVar);
 }
}
sessionStorage.setItem('key',"red_1");
sessionStorage.setItem('lock',"mutex_unlock");

function myfunction_1() {
var elem=document.getElementById("idvid_1");   
       if(sessionStorage.getItem('key')=="green_1")
   {
       elem.webkitExitFullscreen();
       
       elem.load();
      sessionStorage.setItem('lock',"mutex_unlock");     
   }
     if (sessionStorage.getItem('key')=="red_1")
          {          elem.webkitRequestFullScreen();   
    elem.play();
    sessionStorage.setItem('key', "green_1");
         } 
}        


function test_1(){
 if((event.key == "Enter") && ((document.getElementById("ans_1").value == "yes")) && (sessionStorage.getItem('lock')=="mutex_unlock"))
     {  document.getElementById("mem_1").innerHTML= "It was my second year , Hagrid was is trouble we needed to do something..";   
        setTimeout(function(){document.getElementById("mem_1").innerHTML= "Just tap the spiders  and tap back to come out";},3000);
        document.getElementById("idvid_1").setAttribute('poster','anim/spider_1.gif');     
        sessionStorage.setItem('lock',"mutex");  
      }
}
</script>
</div>

<div id="page_28" style="background-image:url(pages/blank.jpg)" onmouseover="background_changer_28(this);">
<script type="text/javascript">
var myVar = setInterval(magic, 3000);
function magic() {
var temp=sessionStorage.getItem('p');
if(temp == "po")
 { document.getElementById("page_28").style.backgroundImage="url('pages/28.jpg')";
