<div id="video_31" style="background-image:url(pages/blank.jpg)">
<span id="mem_31" >
I have a memory I can show you!
</span>
<video poster = "anim/blank.png"  onclick="myfunction_1();" align="top|middle" width="500" height="500"  id="idvid_31">
<source src="videos\troll.mp4" type="video/mp4">
</video>
<input id="ans_31" type="text" onkeypress="test_1()">
<script type="text/javascript">
var myVar = setInterval(magic, 3000);
function magic() {
var temp=sessionStorage.getItem('p');
if(temp == "po")
 { document.getElementById("video_31").style.backgroundImage="url('pages/video31.jpg')";
  clearInterval(myVar);
 }
}

function myfunction_1() {
var elem=document.getElementById("idvid_31");
if(!elem.webkitFullscreenEnabled)
   {
       elem.webkitExitFullscreen();
       elem.load();
   }
     if (elem.webkitRequestFullscreen)
         { elem.webkitRequestFullScreen();
          }
setTimeout(function(){ 
document.getElementById("mem_31").innerHTML= "I have a memory I can show you";
document.getElementById("idvid_31").setAttribute('poster','anim/blank.jpg');
var new_input = document.createElement("input");
new_input.setAttribute("id", "ans_31");
new_input.setAttribute("type","text");
var enclosing = document.getElementById("video_31");
enclosing.appendChild(new_input);
 }, 3000);
document.getElementById("ans_31").addEventListener("keypress",test_1,false);
}

function test_1(){
 if((event.key == "Enter") && ((document.getElementById("ans_31").value == "yes")))
     {  document.getElementById("mem_31").innerHTML= "tap the troll";   
        setTimeout(function(){document.getElementById("mem_31").innerHTML= "ugly nasty thing , ron was lucky enough to cast that spell";},3000);
        document.getElementById("idvid_31").setAttribute('poster','anim/troll.gif');     
        var element = document.getElementById("ans_31");
         element.parentNode.removeChild(element);
  
        }
}
</script>
</div>


