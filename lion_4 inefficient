function myfunction_1() {
var elem=document.getElementById("idvid_14");
if(!elem.webkitFullscreenEnabled)
   {
       elem.webkitExitFullscreen();
       elem.load();
   }
     if (elem.webkitRequestFullscreen)
         { elem.webkitRequestFullScreen();
          }
setTimeout(function(){ 
document.getElementById("mem_14").innerHTML= "this is the author's memory";
document.getElementById("idvid_14").setAttribute('poster','anim/blank.jpg');
var new_input = document.createElement("input");
new_input.setAttribute("id", "ans_14");
new_input.setAttribute("type","text");
var enclosing = document.getElementById("video_14");
enclosing.appendChild(new_input);
 }, 3000);
document.getElementById("ans_14").addEventListener("keypress",test_1,false);
}

function test_1(){
 if((event.key == "Enter") && ((document.getElementById("ans_14").value == "yes")))
     {  document.getElementById("mem_14").innerHTML= "I fought these beasts in my last round of the triwizrd cup in the maze";   
        setTimeout(function(){document.getElementById("mem_14").innerHTML= "Just tap the cup";},3000);
        document.getElementById("idvid_14").setAttribute('poster','anim/cup.gif');     
        var element = document.getElementById("ans_14");
         element.parentNode.removeChild(element);
  
        }
}
</script>
</div>

