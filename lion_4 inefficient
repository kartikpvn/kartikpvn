this is the author's memory
</span>
<video poster = "anim/blank.png"  onclick="myfunction_1();" align="top|middle" width="500" height="500"  id="idvid_7">
<source src="videos\diricrwal.mp4" type="video/mp4">
</video>
<input id="ans_7" type="text" onkeypress="test_1()">
<script type="text/javascript">
var myVar = setInterval(magic, 3000);
function magic() {
var temp=sessionStorage.getItem('p');
if(temp == "po")
 { document.getElementById("video_7").style.backgroundImage="url('pages/video7.jpg')";
  clearInterval(myVar);
 }
}

function myfunction_1() {
var elem=document.getElementById("idvid_7");
if(!elem.webkitFullscreenEnabled)
   {
       elem.webkitExitFullscreen();
       elem.load();
   }
     if (elem.webkitRequestFullscreen)
         { elem.webkitRequestFullScreen();
          }
setTimeout(function(){ 
document.getElementById("mem_7").innerHTML= "this is the author's memory";
document.getElementById("idvid_7").setAttribute('poster','anim/blank.jpg');
var new_input = document.createElement("input");
new_input.setAttribute("id", "ans_7");
new_input.setAttribute("type","text");
var enclosing = document.getElementById("video_7");
enclosing.appendChild(new_input);
 }, 3000);
document.getElementById("ans_7").addEventListener("keypress",test_1,false);
}

function test_1(){
 if((event.key == "Enter") && ((document.getElementById("ans_7").value == "yes")))
     {  document.getElementById("mem_7").innerHTML= "this is a magical bird";   
        setTimeout(function(){document.getElementById("mem_7").innerHTML= "Just tap the bird";},3000);
        document.getElementById("idvid_7").setAttribute('poster','anim/dodo.gif');     
        var element = document.getElementById("ans_7");
         element.parentNode.removeChild(element);
  
        }
}
</script>
</div>

